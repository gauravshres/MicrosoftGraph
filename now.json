{
  "version": 2,
  "name": "MicrosoftGraphs",
  "builds": [{ "src": "bin/www", "use": "@now/node" }],
  "routes": [
    { "src": "/", "dest": "bin/www" },
    { "src": "/auth/signin", "method": "GET", "dest": "bin/www" },
    { "src": "/auth/callback", "method": "POST", "dest": "bin/www" },
    { "src": "/auth/signout", "method": "GET", "dest": "bin/www" }
  ]
}
